import java.util.*;
class palin
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter a and b value");
        int a=sc.nextInt();
        int b=sc.nextInt();
        int i;
        for(i=a;i<=b;i++)
        {
            if(i%2==0)
            {
                System.out.println(i);
            }
        }
    }
}
